{
  "name": "n8n-nodes-zenflo",
  "version": "1.0.3",
  "description": "n8n node for ZenFlo - bidirectional communication with ZenFlo mobile sessions",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "zenflo",
    "webhook",
    "chat",
    "mobile"
  ],
  "license": "MIT",
  "homepage": "https://zenflo.dev",
  "author": {
    "name": "Quinn May",
    "email": "quinn@zenflo.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenflo/zenflo.git"
  },
  "bugs": {
    "url": "https://github.com/zenflo/zenflo/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ZenFloApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ZenFlo/ZenFlo.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.7.1",
    "typescript": "~5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
