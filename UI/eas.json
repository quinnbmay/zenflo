{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "ios": {
        "autoIncrement": true,
        "distribution": "internal",
        "channel": "production"
      },
      "android": {
        "autoIncrement": true,
        "buildType": "apk",
        "channel": "production"
      },
      "env": {
        "APP_ENV": "production"
      }
    },
    "development": {
      "autoIncrement": true,
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_ENV": "development"
      }
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_ENV": "preview"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "steve@bulkovo.com",
        "ascAppId": "126165711",
        "appleTeamId": "466DQWDR8C"
      }
    }
  }
}